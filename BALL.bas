10 WINDOW(-360,-262)-(359,262)
20 PI=3.14159
30 C=PI*2
40 CLS
50 INPUT R
60 CIRCLE(0,0),R
70 PAINT(0,0)
80 FOR I=1 TO 10
90 LOCATE 1,70 :PRINT I
100   P=R*I/150
110   XO=-(R/2-P)*COS(1)
120   YO=(R/2-P)*SIN(1)
130   FOR AO=0 TO 2*PI STEP 1/C
140 LOCATE 1,75 :PRINT INT(AO)
150     A=AO+RND/3
160     X=XO+P*COS(A)*2*I+(RND-RND)*I
170     Y=YO+P*SIN(A)*2*I+(RND-RND)*I
180     IF R<SQR(X*X+Y*Y) THEN 200
190     PRESET(X,Y)
200   NEXT
210 NEXT
220 END
